{
  "extends": "./tsconfig-esm2015.json",
  "compilerOptions": {
    "target": "es5",
    "outDir": "../tmp/esm5",
    "rootDir": "../lib"
  },
  "angularCompilerOptions": {
    "annotateForClosureCompiler": true,
    "strictMetadataEmit": true,
    "skipTemplateCodegen": true,
    "flatModuleOutFile": "ngx-restangular.js",
    "flatModuleId": "ngx-restangular"
  }
}
